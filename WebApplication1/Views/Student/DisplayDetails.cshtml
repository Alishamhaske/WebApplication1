
@{
    ViewData["Title"] = "DisplayDetails";
}

<h1>DisplayDetails</h1>


<table class="table">

    <tr>
        <td>Your ID</td>
        <td>
            @ViewBag.Id
        </td>
    </tr>

    <tr>
        <td>Your Name</td>
        <td>
            @ViewBag.Name
        </td>
    </tr>


    <tr>
        <td>Gender</td>
        <td>
            @ViewBag.Gender
        </td>
    </tr>

    <tr>
        <td>City</td>
        <td>
            @ViewBag.City
        </td>
    </tr>


    <tr>
        <td>Selected Subject</td>
        <td>
            @foreach (string s in ViewBag.Subject)
            {
                <p>@s</p>
            }
        </td>


    </tr>
</table>
